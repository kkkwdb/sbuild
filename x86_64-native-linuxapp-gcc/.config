CONFIG_SRTE_EXEC_ENV="linuxapp"
# SRTE_ARCH values are architecture we compile for. directories in mk/arch/
CONFIG_SRTE_ARCH="x86_64"
# machine can define specific variables or action for a specific board
# SRTE_MACHINE values are architecture we compile for. directories in mk/machine/
CONFIG_SRTE_MACHINE="native"
# The compiler we use.
# SRTE_TOOLCHAIN values are architecture we compile for. directories in mk/toolchain/
CONFIG_SRTE_TOOLCHAIN="gcc"
# Machine forces strict alignment constraints.
CONFIG_SRTE_ARCH_STRICT_ALIGN=n
# Compile to share library
CONFIG_SRTE_BUILD_SHARED_LIB=n
# Use newest code breaking previous ABI
CONFIG_SRTE_NEXT_ABI=y
# Major ABI to overwrite library specific LIBABIVER
CONFIG_SRTE_MAJOR_ABI=
